{
    "name": "extremesensio/psh-route-reader",
    "description": "Help reading the *_ROUTES env var in a Platform.sh env",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Clement Herreman",
            "email": "clement.herreman@sensiogrey.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "platformsh/config-reader": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Sensio\\PshRouteReader\\Tests\\": "tests/",
            "Sensio\\PshRouteReader\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true,
        "platform": {
            "php": "5.6"
        }
    },
    "minimum-stability": "stable"
}
